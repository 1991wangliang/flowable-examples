<?xml version="1.0" encoding="UTF-8"?>
<definitions
        xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
        xmlns:flowable="http://flowable.org/bpmn"
        targetNamespace="Examples">

    <process id="asyncWait">
        <startEvent id="theStart"/>
        <sequenceFlow sourceRef="theStart" targetRef="theTask"/>
        <serviceTask id="theTask" flowable:async="true" flowable:exclusive="false" flowable:class="org.flowable.JobDelegate" />
        <sequenceFlow sourceRef="theTask" targetRef="wait"/>
        <receiveTask id="wait" />
        <sequenceFlow sourceRef="wait" targetRef="theEnd"/>
        <endEvent id="theEnd"/>
    </process>

</definitions>
